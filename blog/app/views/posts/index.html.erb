
<h1>Posts</h1>

<div class="row">
    <% @posts.each do |post| %>
			<div class="col-md-3">
          <%=link_to  image_tag(post.image,style:'width:350px;height:350px'),post%>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
 </div>
    </div>
  <% end %>

